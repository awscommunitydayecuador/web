{
  "version": "1.0",
  "buildSettings": {
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "nodeVersion": "18",
    "environmentVariables": {
      "NODE_ENV": "production",
      "NEXT_PUBLIC_SITE_URL": "https://day.awscommunity.ec"
    },
    "cacheSettings": {
      "paths": [
        "node_modules/**/*",
        ".next/cache/**/*"
      ]
    },
    "performanceSettings": {
      "enableCompression": true,
      "enableMinification": true,
      "enableSourceMaps": false,
      "enableBundleAnalyzer": false
    }
  }
} 